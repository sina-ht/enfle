enfle {
	plugins {
		dir = ../plugins
		ui {
			default = Normal
			normal {
				; normal/render = {normal|double|short|long}
				render = normal
				; magnify = no/bilinear
				magnify_interpolate = bilinear
			}
		}
		video/default = Xlib
		spi/disabled = yes
		player/MPEG_lib/disabled = yes
	}
}
